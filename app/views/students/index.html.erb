<h1>Students#index</h1>
<p>Find me in app/views/students/index.html.erb</p>

<table>
  <% @students.each do |std| %>
    <tr>
      <td><%= link_to std.name, student_path(std)%></td>
      <td><%= link_to "Edit", edit_student_path(std) %></td>
      <td><%= button_to "Delete", student_path(std), method: :delete, data: { turbo_confirm: 'Are you sure?' } %><td>
    </tr>
  <% end %>
  <tr>
    <%= link_to "Add New Student", new_student_path	%>
  </tr>
</table>